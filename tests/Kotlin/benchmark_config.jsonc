{
    // Actions
    "actions": {
        "description": "Spring RestClient Benchmark [Kotlin]",
        "output_filename": "benchmark_output.json",
        "report_filename": "benchmark_report.html",
        "user_class": "HttpUser",
        "user_params": {
            "baseURI": "http://localhost:7070",
            "connectTimeoutMillis": 500,
            "readTimeoutMillis": 2000,
            "debug": false
        },
        "user_actions": {
            "0": "onStart",  // Init
            "1": "GET:posts",
            "2": "GET:comments",
            "3": "GET:todos",
            "99": "onStop"   // Shutdown
        }
    },
    // Workflows using Markov chains
    "workflows": {
        "api-user": {
            "-": {
                "1": 0.40,
                "3": 0.60
            },
            "1": {
                "2": 1.0
            },
            "2": {
                "-": 1.0
            },
            "3": {
                "-": 1.0
            }
        }
    },
    // Benchmarks
    "benchmarks": {
        "onStart": {
            "save_stats": false,
            "scenario_actions": [ {"id": 0} ]
        },
         "REST1": {
            "enabled": true,
            "aps_rate": 10000.0,
            "scenario_actions": [
                {
                    "id": 1
                }
            ],
            "time": {
                "pre_warmup_duration": 30,
                "warmup_duration": 10,
                "benchmark_duration": 30,
                "benchmark_repeat_count": 3
            }
        },
        "REST2": {
            "enabled": true,
            "aps_rate": 10000.0,
            "scenario_actions": [
                {
                    "id": 1, "weight": 10
                },
                {
                    "id": 2, "weight": 40
                },
                {
                    "id": 3, "weight": 50
                }
            ],
            "time": {
                "pre_warmup_duration": 30,
                "warmup_duration": 10,
                "benchmark_duration": 30,
                "benchmark_repeat_count": 3
            }
        },
        "REST3": {
            "enabled": true,
            "aps_rate": 10000.0,
            "scenario_workflow": "api-user",
            "time": {
                "pre_warmup_duration": 30,
                "warmup_duration": 10,
                "benchmark_duration": 30,
                "benchmark_repeat_count": 3
            }
        },
        "onStop": {
            "save_stats": false,
            "scenario_actions": [
                {
                    "id": 99
                }
            ]
        }
    },
    // Contexts
    "contexts": {
        "Context-1": {
            "enabled": true,
            "num_users": 128,
            "num_threads": 2
        }
    }
}
